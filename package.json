{
  "name": "connect-mssql-v2",
  "version": "3.1.4",
  "description": "MS SQL Server session store for Express Session",
  "main": "dist/src/store",
  "types": "dist/src/store.d.ts",
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "build": "npx tsc",
    "test": "jest ./dist/test",
    "lint": "npx eslint src --ext .ts",
    "clean": "npx rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JLuboff/connect-mssql-v2.git"
  },
  "keywords": [
    "sql",
    "express",
    "session",
    "store",
    "mssql"
  ],
  "author": "Jason Luboff",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JLuboff/connect-mssql-v2/issues"
  },
  "homepage": "https://github.com/JLuboff/connect-mssql-v2#readme",
  "dependencies": {
    "mssql": "^8.1.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^28.1.6",
    "@types/mssql": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "express-session": "^1.17.3",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-jasmine2": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "files": [
    "dist/src/store.js",
    "dist/src/store.d.ts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --quiet"
    }
  },
  "lint-staged": {
    ".{ts}": [
      "prettier --write"
    ]
  }
}
